{
  "name": "prep-vault",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "vitest",
    "test:e2e": "playwright test",
    "setup": "npm ci && npx tailwindcss init -p || true && npm run prisma:generate || true",
    "prisma:generate": "prisma generate || true"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@prisma/client": "^5.10.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "25.0.3",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.45.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "prisma": "^5.10.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.0",
    "vitest": "^1.2.0"
  }
}
